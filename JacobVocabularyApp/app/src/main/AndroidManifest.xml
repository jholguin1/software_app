<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="holguin.jacobvocabularyapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".setContentViewActivity"></activity>
        <activity android:name=".ActivityVocabulary"></activity>
        <activity android:name=".XmlVocabulary"></activity>
        <activity android:name=".IntentVocabulary"></activity>
        <activity android:name=".onClickListenerVocabulary"></activity>
        <activity android:name=".onCreateVocabulary"></activity>
    </application>

</manifest>